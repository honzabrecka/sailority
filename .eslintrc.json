{
  "env": {
      "node": true,
      "browser": true,
      "es6": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "semi": [2, "never"],
    "no-shadow": 0,
    "no-unused-expressions": 0,
    "no-use-before-define": 0,
    "class-methods-use-this": 0,
    "import/no-extraneous-dependencies": 0,
    "react/sort-comp": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": 0,
    "react/no-multi-comp": 0
  }
}
